\f1\b0 \cf2 \
\pard\tx220\tx720\pardeftab720\li720\fi-720\partightenfactor0
\ls11\ilvl0
\f3 \cf2 \cb1 \up0 \nosupersub \ulnone {\listtext	\uc0\u8226 	}
